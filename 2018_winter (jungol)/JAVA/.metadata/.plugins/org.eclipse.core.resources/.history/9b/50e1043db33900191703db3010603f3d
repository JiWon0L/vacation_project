import java.util.Scanner;

public class Main {
	static int n;

	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		for (int i = 0; i < 9; i++) {
			n = scan.nextInt();
			System.out.println(funtion(n));
		}
	}

	public static int funtion(int n) {
		return (int) Math.pow(n, 2);
	}
}
