import java.util.Scanner;

public class Main {
	static int n;
	static int[] array;

	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		n = scan.nextInt();
		array = new int[n];
		for (int i = 0; i < n; i++) {
			array[i] = 1;
		}
		funtion();
	}

	public static void funtion() {
		for (int i = 0; i < n; i++) {
			for (int j = 0; j < n; j++) {
				for (int k = 0; k < n; j++) {
					System.out.print(array[k] + " ");
				}
				array[k]++;
				System.out.println();

			}

		}
	}
}
