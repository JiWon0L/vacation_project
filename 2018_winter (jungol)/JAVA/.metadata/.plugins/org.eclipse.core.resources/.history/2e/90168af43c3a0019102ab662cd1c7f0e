import java.util.Scanner;

public class Main {
	static int n;
	static int[] array;

	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		n = scan.nextInt();
		array = new int[n];
		for (int i = 0; i < n; i++) {
			array[i] = 1;
		}
		funtion();
	}

	public static void funtion() {
		for (int i = 6; i > 0 ; i--) {
			for (int j = 0; j < i; j++) {
				System.out.print(array[j] + " ");
				if (array[j] == 6) {
					array[j - 1]++;
					array[j] = array[j - 1];
				}
			}
			
			
			System.out.println();

		}
	}
}
